      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "df_bowl = pd.read_csv(\"iplbowler.csv\",encoding='windows-1252')\n",
        "df_bowl.columns"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "AB_8wZSMNfRS",
        "outputId": "28ca8ab7-b19a-4ff6-a96f-87f4f1ca4b2d"
